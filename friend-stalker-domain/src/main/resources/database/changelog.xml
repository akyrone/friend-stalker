<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                                       http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

	<changeSet author="tomas" id="1">
		<createTable tableName="Users">
			<column name="userId" autoIncrement="true" type="BIGINT(20)">
				<constraints primaryKey="true" nullable="false" />
			</column>
			<column name="full_name" type="VARCHAR(30)"></column>
			<column name="uid" type="VARCHAR(20)"></column>
			<column name="tokken" type="VARCHAR(20)"></column>
			<column name="valid_till" type="TIMESTAMP"></column>
		</createTable>
	</changeSet>
</databaseChangeLog>
                                       