<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
                                 http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0" xmlns="http://java.sun.com/xml/ns/persistence">
    <persistence-unit name="friendStalker" transaction-type="RESOURCE_LOCAL">
    	<class>com.itomkinas.friendStalker.domain.entity.UserEntity</class>
    	<class>com.itomkinas.friendStalker.domain.entity.Stalking</class>
    	<class>com.itomkinas.friendStalker.domain.entity.Connection</class>
    	<class>com.itomkinas.friendStalker.domain.entity.OnlinePresence</class>
    	<properties>
			<property name="hibernate.dialect" value="org.hibernate.dialect.HSQLDialect"/>
      	</properties>
    </persistence-unit>
</persistence>